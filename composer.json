{
	"name": "mediadreams/md_news_clickcount",
	"description": "With this TYPO3 extension you are able to count views of ext:news records. You can display a list with most viewed news and delete the statistics automatically with a scheduler task.",
	"license": "GPL-2.0-or-later",
	"type": "typo3-cms-extension",
	"keywords": [
		"TYPO3",
		"TYPO3 CMS",
		"extension",
		"news",
		"tx_news",
		"count",
		"views"
	],
	"authors": [
		{
			"name": "Christoph Daecke",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/cdaecke/md_news_clickcount/issues",
		"source": "https://github.com/cdaecke/md_news_clickcount"
	},
	"require": {
		"georgringer/news": ">=11.0 <15.0",
		"typo3/cms-core": ">=12.0 <14.0"
	},
	"autoload": {
		"psr-4": {
			"Mediadreams\\MdNewsClickcount\\": "Classes"
		}
	},
	"config": {
		"allow-plugins": {
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "md_news_clickcount"
		}
	}
}
